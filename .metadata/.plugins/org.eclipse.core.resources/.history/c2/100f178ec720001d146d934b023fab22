package sm.api.pojo;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.Map;

import com.fasterxml.jackson.annotation.JsonAnyGetter;
import com.fasterxml.jackson.annotation.JsonAnySetter;
import com.fasterxml.jackson.databind.ObjectMapper;


public class DivisionContainer{
	
	private Map<String, DivisionData> div1Data = new HashMap<>();
	//private Map<String, DivisionData> div2Data = new HashMap<>();

	@JsonAnySetter
    public void setDiv1(String name, Object value) {
		new ObjectMapper().convertValue(value, DivisionData.class);
		div1Data.put(name, (DivisionData) value);
    }
	
//	@JsonAnySetter
//    public void setDiv2(String name, Object value) {
//		div2Data.put(name, (DivisionData) value);
//    }
	
}